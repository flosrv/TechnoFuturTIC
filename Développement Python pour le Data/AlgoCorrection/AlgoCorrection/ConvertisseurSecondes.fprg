    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="TotSec, Jour, Heure, Minute, Seconde" type="Integer" array="False" size=""/>
            <output expression="&quot;Entrez un nombre de secondes:&quot;" newline="True"/>
            <input variable="TotSec"/>
            <assign variable="Jour" expression="TotSec / 86400"/>
            <assign variable="Heure" expression="(TotSec % 86400) / 3600"/>
            <assign variable="Minute" expression="((TotSec % 86400)% 3600)/60"/>
